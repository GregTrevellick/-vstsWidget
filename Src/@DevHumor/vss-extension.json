{
    "manifestVersion": 1,
    "id": "vsts-extensions-tweets-devhumor",
    "version": "1.1.2",
    "name": "@DevHumor",
    "publisher": "GregTrevellick",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "dev humor tweets",
    "icons": {
        "default": "app/Images/DefaultIcon.png"
    },
    "categories": [
        "Collaborate"
    ],
    "tags": [
        "dashboard",
        "tweet",
        "twitter",
        "devhumor",
        "widget"
    ],
    "links": {
        "home": {
            "uri": "https://marketplace.visualstudio.com/items?itemName=GregTrevellick.YourWidgetId"
        },
        "repository": {
            "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
        },
        "issues": {
            "uri": "https://github.com/GregTrevellick/VsixTwitterWidget/issues"
        }
    },
    "screenshots": [
        {
            "path": "artefacts/Screenshot.png"
        }
    ],
    "branding": {
        "color": "black",
        "theme": "dark"
    },
    "contributions": [
        {
            "id": "VSSRegisterName_Tweet@DevHumor",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],

            "properties": {
                "name": "dev humor Your Widget Name (Contributions)",
                "description": "dev humor Your brief free-text widget contributions properties description",
                "catalogIconUrl:": "app/Images/DefaultIcon.png",
                "previewImageUrl": "app/Images/DefaultIcon.png",
                "uri": "app/dashboard-widget.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 3
                    }
                ],

                "supportedScopes": [ "project_team" ]
            }
        }
    ],
    "files": [
        {
            "path": "app/dashboard-widget.html",
            "addressable": true
        },
        {
            "path": "app/Images",
            "addressable": true
        },
        {
            "path": "app/Scripts",
            "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "artefacts/Overview.md"
        },

        "license": {
            "path": "artefacts/Licence.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
    }
}