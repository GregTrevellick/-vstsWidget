{
    "manifestVersion": 1,
    "id": "vsts-extensions-tweets-AzureDevOps",
    "version": "1.1.193",
    "name": "@AzureDevOps",
    "publisher": "GregTrevellick",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "description": "Display @AzureDevOps tweets in your project dashboard",
    "icons": {
        "default": "app/Images/DefaultIcon.png"
    },
    "categories": [
        "Collaborate"
    ],
    "tags": [
        "dashboard",
        "tweet",
        "twitter",
        "AzureDevOps",
        "widget"
    ],
  "links": {
    "home": {
      "uri": "https://marketplace.visualstudio.com/items?itemName=GregTrevellick.vsts-extensions-tweets-AzureDevOps"
    },
    "repository": {
      "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
    },
    "issues": {
      "uri": "https://github.com/GregTrevellick/VsixTwitterWidget/issues"
    },
    "support": {
      "uri": "https://github.com/GregTrevellick/VsixTwitterWidget/issues"
    },
    "getstarted": {
      "uri": "https://gregtrevellick.github.io/More/"
    },
    "learn": {
      "uri": "https://gregtrevellick.github.io/More/"
    },
    "privacypolicy": {
      "uri": "https://gregtrevellick.github.io/Azure-DevOps-Widget-Privacy-Policy/"
    }
  },
    "screenshots": [
        {
            "path": "artefacts/Screenshot.png"
        }
    ],
    "branding": {
        "color": "rgb(1, 122, 215)",
        "theme": "dark"
    },
    "contributions": [
        {
            "id": "VSSRegisterName_Tweet@AzureDevOps",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
          "properties": {
            "name": "AzureDevOps Tweets",
            "description": "Display @AzureDevOps tweets in your project dashboard",
            "catalogIconUrl:": "app/Images/DefaultIcon.png",
            "previewImageUrl": "app/Images/DefaultIcon.png",
            "uri": "app/dashboard-widget.html",
            "supportedSizes": [
              {
                "rowSpan": 2,
                "columnSpan": 2
              },
              {
                "rowSpan": 2,
                "columnSpan": 3
              }
            ],
            "supportedScopes": [ "project_team" ]
          }
        }
    ],
    "files": [
        {
            "path": "app/dashboard-widget.html",
            "addressable": true
        },
        {
            "path": "app/Images",
            "addressable": true
        },
        {
            "path": "app/Scripts",
            "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "artefacts/Overview.md"
        },
        "license": {
            "path": "artefacts/Licence.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
    },
    "public" :  true
}