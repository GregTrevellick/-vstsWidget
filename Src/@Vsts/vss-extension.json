{
    "manifestVersion": 1,
    "id": "vsts-extensions-tweets-vsts",
    "version": "1.0.19",
    "name": "@VSTS",
    "description": "Display tweets in your project dashboard",
    "publisher": "GregTrevellick",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "icons": {
        "default": "images/Icon_twitter_avatar_128x128.png"
    },
    "categories": [
        "Collaborate"
    ],
    "tags": [
        "dashboard",
        "tweet",
        "twitter",
        "vsts",
        "widget"
    ],
    "contributions": [
        {
            "id": "VSSRegisterName_Tweet@Vsts",
            "type": "ms.vss-dashboards-web.widget",
            "targets": [
                "ms.vss-dashboards-web.widget-catalog"
            ],
            "properties": {
                "name": "VstsTweets",
                "description": "Display tweets in your project dashboard",
                "catalogIconUrl:": "images/Icon_twitter_avatar_128x128.png",
                "previewImageUrl": "images/Icon_twitter_avatar_128x128.png",
                "uri": "Widget.html",
                "supportedSizes": [
                    {
                        "rowSpan": 2,
                        "columnSpan": 3
                    }
                ],
                "supportedScopes": [ "project_team" ]
            }
        }
    ],
    "files": [
        {
            "path": "Widget.html",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "content": {
        "details": {
            "path": "Overview.md"
        },
        "license": {
            "path": "LICENSE.md"
        }
    },
    "links": {
        "home": {
            "uri": "https://marketplace.visualstudio.com/items?itemName=GregTrevellick.vsts-extensions-tweets-vsts"
        },
        "repository": {
            "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
        },
        "issues": {
            "uri": "https://github.com/GregTrevellick/VsixTwitterWidget/issues"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/GregTrevellick/VsixTwitterWidget"
    },
    "branding": {
        "color": "rgb(1, 122, 215)",
        "theme": "dark"
    },
    "galleryFlags": [
        "Public"
    ]    
}